<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
         
          <display-name>Meliora1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
         
    <servlet>
        <servlet-name>AddDiscipline</servlet-name>
        <servlet-class>AddDisciplineServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddDiscipline</servlet-name>
        <url-pattern>/AddDiscipline</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EditDiscipline</servlet-name>
        <servlet-class>EditDisciplineServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditDiscipline</servlet-name>
        <url-pattern>/EditDiscipline</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteDiscipline</servlet-name>
        <servlet-class>DeleteDisciplineServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteDiscipline</servlet-name>
        <url-pattern>/DeleteDiscipline</url-pattern>
    </servlet-mapping>
    
    <servlet>
    <servlet-name>DisciplineServlet</servlet-name>
    <servlet-class>DisciplineServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>DisciplineServlet</servlet-name>
    <url-pattern>/DisciplineServlet</url-pattern>
</servlet-mapping>
</web-app>